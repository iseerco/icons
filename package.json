{
  "name": "@iseer/icons",
  "version": "0.2.0",
  "description": "Iseer's custom icon library with 9 variants (straight/rounded Ã— regular/thin/bold/solid + duotone chubby)",
  "author": "Iseer",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/iseerco/icons"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./straight/regular": {
      "types": "./dist/straight/regular/index.d.ts",
      "import": "./dist/straight/regular/index.mjs",
      "require": "./dist/straight/regular/index.js"
    },
    "./straight/thin": {
      "types": "./dist/straight/thin/index.d.ts",
      "import": "./dist/straight/thin/index.mjs",
      "require": "./dist/straight/thin/index.js"
    },
    "./straight/bold": {
      "types": "./dist/straight/bold/index.d.ts",
      "import": "./dist/straight/bold/index.mjs",
      "require": "./dist/straight/bold/index.js"
    },
    "./straight/solid": {
      "types": "./dist/straight/solid/index.d.ts",
      "import": "./dist/straight/solid/index.mjs",
      "require": "./dist/straight/solid/index.js"
    },
    "./rounded/regular": {
      "types": "./dist/rounded/regular/index.d.ts",
      "import": "./dist/rounded/regular/index.mjs",
      "require": "./dist/rounded/regular/index.js"
    },
    "./rounded/thin": {
      "types": "./dist/rounded/thin/index.d.ts",
      "import": "./dist/rounded/thin/index.mjs",
      "require": "./dist/rounded/thin/index.js"
    },
    "./rounded/bold": {
      "types": "./dist/rounded/bold/index.d.ts",
      "import": "./dist/rounded/bold/index.mjs",
      "require": "./dist/rounded/bold/index.js"
    },
    "./rounded/solid": {
      "types": "./dist/rounded/solid/index.d.ts",
      "import": "./dist/rounded/solid/index.mjs",
      "require": "./dist/rounded/solid/index.js"
    },
    "./duotone/chubby": {
      "types": "./dist/duotone/chubby/index.d.ts",
      "import": "./dist/duotone/chubby/index.mjs",
      "require": "./dist/duotone/chubby/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "optimize": "node scripts/optimize-svgs.js",
    "generate": "node scripts/generate-components.js",
    "build": "npm run generate && cross-env NODE_OPTIONS=--max-old-space-size=8192 tsup",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "svgo": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "icons",
    "react-icons",
    "svg-icons",
    "iseer",
    "ui-icons",
    "react-components",
    "straight-icons",
    "rounded-icons",
    "icon-library"
  ],
  "sideEffects": false,
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "dotenv": "^17.2.3",
    "glob": "^13.0.0",
    "mime-types": "^3.0.2",
    "p-limit": "^7.2.0"
  }
}
